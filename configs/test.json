{
    "name": "hifigan",
    "n_gpu": 1,
    "arch": {
        "type": "HiFiGANModel",
        "args": {
            "generator_args": {
                "h_u": 512,
                "k_u": [16, 16, 4, 4],
                "k_r": [3, 7, 11],
                "D_r": [
                    [[1, 1], [3, 1], [5, 1]], 
                    [[1, 1], [3, 1], [5, 1]], 
                    [[1, 1], [3, 1], [5, 1]]
                ]
            },
            "mpd_periods": [2, 3, 5, 7, 11],
            "msd_args": {
                "channels_list": [128, 128, 256, 512, 1024, 1024, 1024, 1],
                "kernels": [15, 41, 41, 41, 41, 41, 5, 3],
                "strides": [1, 2, 2, 4, 4, 1, 1, 1],
                "groups_list": [1, 4, 16, 16, 16, 16, 1, 1]
           }
        }
    },
    "data": {
        "test": {
            "batch_size": 1,
            "num_workers": 1,
            "datasets": [
                {
                    "type": "LJSpeechDataset",
                    "args": {
                        "dir": "test_model/test_data"
                    }
                }
            ]
        }
    }
}
